<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            default="false"
            extensions="ag4"
            icon="icons/AllegroGraph.gif"
            id="com.franz.agraph.editor"
            launcher="org.topbraidcomposer.editors.ResourceEditorLauncher"
            name="TopBraid (AllegroGraph 4 Database)"/>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.topbraidcomposer.wizards"
            class="com.franz.agraph.tbc.wizards.create.CreateAllegroWizard"
            icon="icons/AllegroGraph.gif"
            id="org.topbraidcomposer.allegro.wizards.create"
            name="RDF/OWL AllegroGraph 4 Database Connection"/>
   </extension>
   <extension
         point="org.topbraidcomposer.wizards.exportWizardPlugins">
      <exportWizardPlugin
            class="com.franz.agraph.tbc.wizards.export.AllegroExportWizardPlugin"
            extension="ag4"/>
   </extension>

</plugin>
